<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <LangVersion>preview</LangVersion>
        <Nullable>enable</Nullable>
        <TargetFrameworks>netstandard2.0;net8.0;net9.0;net10.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <LangVersion>preview</LangVersion>

        <Authors>KryKom</Authors>
        <Company>KryKom</Company>
        <PackageProjectUrl>https://github.com/KryKomDev/Metriks</PackageProjectUrl>
        <PackageLicense>https://github.com/KryKomDev/Metriks/blob/main/LICENSE</PackageLicense>
        <RepositoryUrl>https://github.com/KryKomDev/Metriks</RepositoryUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageIcon>KKNoise.png</PackageIcon>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>A multidimensional dynamic array library for C#.</Description>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../README.md" Pack="true" PackagePath=""/>
        <None Include="../Assets/KKNoise.png" Pack="true" PackagePath=""/>
    </ItemGroup>
</Project>
